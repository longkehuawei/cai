<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TRecyclerView">
        <attr name="isReverse" format="boolean" />
        <attr name="isRefreshable" format="boolean" />
        <attr name="needHint" format="boolean" />
        <attr name="itemType" format="reference" />
        <attr name="headType" format="reference" />
        <attr name="footType" format="reference" />
    </declare-styleable>

    <attr name="backgroundCard" format="color" />

    <attr name="textColorBg" format="color" />
    <attr name="themeType" format="string" />
    <attr name="themeIcon" format="reference" />
    <attr name="colorWhite" format="color" />
    <attr name="lineColor" format="color" />
    <attr name="radius" format="dimension" />
    <attr name="strokeWidth" format="dimension" />
    <attr name="circleColor" format="color" />
    <attr name="ringColorRed" format="color" />
    <attr name="ringColorYellow" format="color" />
    <attr name="sidebuffer" format="integer" />
    <attr name="activeColor" format="color" />
    <attr name="inactiveColor" format="color" />
    <attr name="radius2" format="dimension" />
    <attr name="centered" format="boolean" />
    <attr name="fadeOut" format="integer" />
    <attr name="inactiveType">
        <flag name="stroke" value="0x00000000" />
        <flag name="fill" value="0x00000001" />
    </attr>
    <attr name="activeType">
        <flag name="stroke" value="0x00000000" />
        <flag name="fill" value="0x00000001" />
    </attr>
    <attr name="circleSeparation" format="dimension" />
    <attr name="activeRadius" format="dimension" />
    <attr name="civ_border_width" format="dimension" />
    <attr name="civ_border_color" format="color" />
    <attr name="civ_border_overlay" format="boolean" />
    <attr name="civ_fill_color" format="color" />
    <attr name="duration" format="integer" />
    <attr name="stroke_width" format="dimension" />
    <attr name="color_tick" format="color" />
    <attr name="color_checked" format="color" />
    <attr name="color_unchecked" format="color" />
    <attr name="color_unchecked_stroke" format="color" />
    <attr name="titlePadding" format="dimension" />
    <attr name="clipPadding" format="dimension" />
    <attr name="textColor" format="reference|color" />
    <attr name="textSizeNormal" format="dimension" />
    <attr name="textSizeSelected" format="dimension" />
    <attr name="footerLineHeight" format="dimension" />
    <attr name="footerColor" format="color" />
    <attr name="footerTriangleHeight" format="dimension" />

</resources>